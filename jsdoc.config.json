{
    "opts": {
        "template": "node_modules/braintree-jsdoc-template",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    },
    "tags": {
        "allowUnknownTags": ["optional"]
    },
    "plugins": [
        "node_modules/better-docs/typescript"
    ],
    "source": {
        "include": ["src"],
        "includePattern": "\\.(jsx|js|ts|tsx)$"
    },
    "templates": {
        "referenceTitle": "Spb Unofficial Wrapper",
        "disableSort": false,
        "collapse": true,
        "resources": {
            "Github repository": "https://github.com/gkoniaris/spb-unofficial-wrapper/"
        },
        "meta": {
            "title": "Spb Unofficial Wrapper",
            "description": "Lorem picsum",
            "keyword": "scrapingbee nodejs module"
        }
    }
}